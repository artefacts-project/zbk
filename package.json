{
  "name": "zbk",
  "version": "0.0.1",
  "description": "A monorepo with prototypes and libraries for @artefacts project",
  "scripts": {
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artefacts-project/zbk.git"
  },
  "keywords": [
    "Zettelkasten"
  ],
  "author": "Predrag Vujic",
  "bugs": {
    "url": "https://github.com/artefacts-project/zbk/issues"
  },
  "homepage": "https://github.com/artefacts-project/zbk#readme",
  "workspaces": [
    "prototypes/*",
    "libraries/*",
    "apps/*"
  ],
  "private": true,
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.7.7",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.13",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.29.0",
    "eslint": "^9.13.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^6.2.4",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "vitest": "^2.1.3",
    "vue-tsc": "^2.1.6"
  }
}
